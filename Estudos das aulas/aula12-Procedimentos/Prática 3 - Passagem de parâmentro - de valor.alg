algoritmo "semnome"
// Função :
// Autor :
// Data : 17/05/2022
// Seção de Declarações 
var
   x, y: inteiro
procedimento soma(A, B: inteiro)
inicio
      escreval("Recebi o valor", A)
      escreval("Recebi o valor", B)
      escreva("A soma entre os dois valores é ", A+B)
fimprocedimento
inicio
      x<-5
      y<-3
      soma(x, y)
// Seção de Comandos 
fimalgoritmo
