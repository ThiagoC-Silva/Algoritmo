algoritmo "semnome"
// Função :
// Autor :
// Data : 18/05/2022
// Seção de Declarações 
var
   v: vetor[1..6] de inteiro
   c: inteiro
inicio
      para c:=1 ate 6 faca
           escreva("Digite o", c, "° valor:")
           leia(v[c])
      fimpara
      para c:=1 ate 6 faca
           escreva(v[c])
      fimpara
// Seção de Comandos 
fimalgoritmo
